  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "biome": "biome",
    "biome:ci": "biome ci --reporter=github",
    "lint": "biome lint",
    "lint:vscode": "biome lint --reporter=github",
    "lint:fix": "biome lint --write",
    "format": "biome format --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@storybook/addon-essentials": "^8",
    "@storybook/addon-interactions": "^8",
    "@storybook/addon-links": "^9",
    "@storybook/addon-onboarding": "^9",
    "@storybook/blocks": "^8",
    "@storybook/react": "^9",
    "@storybook/react-vite": "^9",
    "@storybook/test": "^8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "ra-data-json-server": "^5",
    "react-admin": "^5",
    "react-icons": "^5.5.0",
    "storybook": "^8",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^6.3.5",
    "vitest": "^3.2.0",
    "vitest-browser-react": "^0.2.0"
  }
